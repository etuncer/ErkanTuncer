import java.util.ArrayList;

public class FibonacciNumbers {


	public static void main(String[] args) {

			ArrayList<Integer> fibSequence = new ArrayList<Integer>();
			fibSequence.add(0);
			fibSequence.add(1);
			fibSequence.add(2);
			for (int i=2; i<99; i++) {
				int newElement = fibSequence.get(i)+fibSequence.get(i-1);
				fibSequence.add(newElement);
			}
		
		System.out.println(fibSequence);
		System.out.println("Number of Fibonaccis in this sequece is "+fibSequence.size());

	}
	
	
}
